{
  "name": "jsproptest",
  "version": "0.5.1",
  "description": "Javascript Property-based Testing",
  "author": "Won-Wook Hong",
  "license": "MIT",
  "keywords": [
    "property-based-testing"
  ],
  "repository": {
    "type": "git",
    "url": "http://github.com/kindone/jsproptest.git"
  },
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "tsup src/index.ts --dts",
    "test": "jest --verbose",
    "lint": "eslint src",
    "prepare": "yarn build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "peerDependencies": {},
  "husky": {
    "hooks": {
      "pre-commit": "eslint src"
    }
  },
  "prettier": {
    "tabWidth": 4,
    "printWidth": 120,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "lint": "eslint src",
  "module": "dist/tsproptest.esm.js",
  "size-limit": [
    {
      "path": "dist/tsproptest.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/tsproptest.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@eslint/js": "^9.18.0",
    "@jest/globals": "^29.7.0",
    "@size-limit/preset-small-lib": "^4.9.1",
    "@types/jest": "^29.5.14",
    "@typescript-eslint/eslint-plugin": "^8.20.0",
    "@typescript-eslint/parser": "^8.20.0",
    "eslint": "^9.18.0",
    "husky": "^4.3.6",
    "jest": "^29.7.0",
    "size-limit": "^4.9.1",
    "ts-jest": "^29.2.5",
    "tslib": "^2.8.1",
    "tsup": "^8.3.5",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0"
  },
  "dependencies": {
    "@types/random-js": "^2.0.0",
    "rand-seed": "^1.0.0",
    "random-js": "^2.1.0"
  }
}
